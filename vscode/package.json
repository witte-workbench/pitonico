{
  "name": "pitonico",
  "displayName": "Pitonico",
  "description": "VSCode support for Pitonico code.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [{
      "id": "pitonico",
      "aliases": ["Pitonico", "pitonico"],
      "extensions": [".pi"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "pitonico",
      "scopeName": "source.pitonico",
      "path": "./syntaxes/pitonico.tmLanguage.json"
    }],
    "commands": [
      {
        "command": "pitonico.runPi",
        "title": "Run Pi File"
      },
      {
        "command": "pitonico.pyToPi",
        "title": "EN to ES"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "pitonico.runPi",
          "when": "resourceLangId == pitonico",
          "group": "navigation"
        },
        {
          "command": "pitonico.pyToPi",
          "when": "resourceLangId == pitonico",
          "group": "navigation"
        }
      ]
    }
  }
}
